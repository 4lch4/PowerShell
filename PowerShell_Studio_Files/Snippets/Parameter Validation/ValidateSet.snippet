<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>ValidateSet</Title>
			<Shortcut>ValidateSet</Shortcut>
			<Description>Limits the valid values of the parameter to the specified set. This attribute works like an enumerator.</Description>
			<Author>SAPIEN Technologies, Inc.</Author>
			<HelpUrl>http://msdn.microsoft.com/en-us/library/system.management.automation.validatesetattribute(v=vs.85).aspx</HelpUrl>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="True">
					<ID>ValidValues</ID>
					<ToolTip>Enter a comma-separated list of valid values.</ToolTip>
					<Function></Function>
					<Default>ValidValues</Default>
				</Literal>
			</Declarations>
			<Code Language="PowerShell">
				<![CDATA[[ValidateSet($ValidValues$)]]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>