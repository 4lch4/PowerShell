<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>AllowEmptyString</Title>
			<Shortcut>AllowEmptyString</Shortcut>
			<Description>Permits an empty string as the value of a mandatory parameter.</Description>
			<Author>SAPIEN Technologies, Inc.</Author>
			<HelpUrl>http://msdn.microsoft.com/en-us/library/system.management.automation.allowemptystringattribute(v=vs.85).aspx</HelpUrl>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Code Language="PowerShell">
				<![CDATA[[AllowEmptyString()]]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>