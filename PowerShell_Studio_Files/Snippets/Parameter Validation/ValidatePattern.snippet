<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>ValidatePattern</Title>
			<Shortcut>ValidatePattern</Shortcut>
			<Description>Requires that parameter values match the specified regular expression.</Description>
			<Author>SAPIEN Technologies, Inc.</Author>
			<HelpUrl>http://msdn.microsoft.com/en-us/library/system.management.automation.validatepatternattribute(v=vs.85).aspx</HelpUrl>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="True">
					<ID>RegEx</ID>
					<ToolTip>Enter a regular expression. Parameter values must match this expression.</ToolTip>
					<Function></Function>
					<Default>RegEx</Default>
				</Literal>
			</Declarations>
			<Code Language="PowerShell">
				<![CDATA[[ValidatePattern($RegEx$)]]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>